# Environment Variables for SentinelAPI Backend

# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/sentinelapi

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRE=7d

# CORS Configuration
FRONTEND_URL=http://localhost:3000

# Proxy Configuration
PROXY_TARGET_URL=https://jsonplaceholder.typicode.com
PROXY_TIMEOUT=30000
ALLOWED_PROXY_TARGETS=jsonplaceholder.typicode.com,api.github.com,reqres.in,localhost,127.0.0.1

# Other Settings
LOG_LEVEL=info

PROXY_TARGET_URL=https://jsonplaceholder.typicode.com
PROXY_TIMEOUT=30000
ALLOWED_PROXY_TARGETS=jsonplaceholder.typicode.com,api.github.com,localhost